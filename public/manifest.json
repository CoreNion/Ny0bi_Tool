{
  "name": "Ny0bi Tool",
  "version": "2.1.0",
  "manifest_version": 3,
  "description": "N予備校をダークモードにするChrome拡張機能。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Ny0bi Toolの設定",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.nnn.ed.nico/*",
        "*://nnn.ed.nico/*"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://www.nnn.ed.nico/*",
    "*://nnn.ed.nico/*"
  ]
}