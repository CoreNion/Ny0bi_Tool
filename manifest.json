{
  "name": "Ny0bi Tool",
  "version": "0.2",
  "manifest_version": 2,
  "description": "N予備校をダークモードにするChrome拡張機能。",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.nnn.ed.nico/*",
        "*://nnn.ed.nico/*"
      ],
      "all_frames": true,
      "js": [
        "library/jquery-3.5.1.min.js",
        "afterEvent.js",
        "afterLoad.js"
      ]
    }
  ],
  "permissions": [
    "*://www.nnn.ed.nico/*",
    "*://nnn.ed.nico/*"
  ]
}